{
  "name": "VSCodeEnvironmentViewer",
  "displayName": "Env Editor",
  "description": "Simple .env file editor",
  "version": "0.0.1",
  "publisher": "aydindie",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/muhammetaydinn/env-viewer"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:envEditor.editor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "envEditor.editor",
        "displayName": "Env Editor",
        "selector": [
          {
            "filenamePattern": "*.env"
          },
          {
            "filenamePattern": ".env*"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "env-editor": "file:",
    "env-viewer": "file:",
    "VSCodeEnvironmentViewer": "file:",
    "VSCodeEnvViewer": "file:"
  }
}
